<h1>Financial Data Graphing</h1>

<form class="form-inline" id="myForm">
  <div class="form-group">
    <label for="exampleInputName2">Starting Date</label>
    <input type="date" class="form-control" id="start" name="startingDate" placeholder="Starting Date">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail2">Ending Date</label>
    <input type="date" class="form-control" id="end" name="endingDate" placeholder="Ending Date">
  </div>
  <select class="form-control" id="currency">
    <option value="USD">USD</option>
    <option value="EUR">EUR</option>
    <%# <option>3</option>
    <option>4</option>
    <option>5</option>%>
  </select>
  <button type="submit" class="btn btn-default">Search Data</button>
</form>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Minimun Value</h3>
  </div>
  <div class="panel-body" id="minVal">
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Maximun Value</h3>
  </div>
  <div class="panel-body" id="maxVal">
  </div>
</div>
<canvas id="canvas" width="300" height="300"></canvas>
